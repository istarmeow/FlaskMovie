    "svg-inline-loader": "0.8.0",
    "template-string-optimize-loader": "^2.2.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.12.0",
    "webpack-cli": "3.0.4",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "axios": "0.21.1",
    "balloon-css": "^0.5.0",
    "promise-polyfill": "8.0.0"
